{
   "manifest_version": 2,
   "name": "Expander",
   "version" : "1.0",

   "browser_action": {
      "default_icon": "zoomin.png",
	  "default_title": "Expander"
   },
   
   "background": {
     "scripts": ["background.js","thirdParty/jquery-2.2.0.min.js"],
     "persistent": false
    },

   "default_locale": "en_US",
   "icons": {
      "128": "zoomin.png"
   },

   "permissions": [ "tabs",
					"storage",
   					"<all_urls>",
					"http://*/"
				  ]
}
